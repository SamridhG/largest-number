#include<stdio.h>
int max(int a,int b)
{
    if(a>b)
    return a;
    else 
    return b;
}
int largest_num(int a,int b,int c,int d)
{
    int f=max(a,max(b,max(c,d)));
    return f;
}
int main()
{
    int a,b,c,d,e;
    scanf("%d%d%d%d",&a,&b,&c,&d);
    e =largest_num(a,b,c,d);
    printf("%d",e);
}
